{
  "name" : "EncryptMessages",
  "version" : "0.3",
  "description" : "Encrypt your VK messages",
  "background" : { "scripts": ["getId.js", "getKey.js" ,"background.js"] },
  "permissions" : [
    "contextMenus",
    "storage",
    "tabs",
    "http://vk.com/",
    "https://vk.com/"
   ],
  "minimum_chrome_version" : "6.0.0.0",
  "icons" : {
    "16" : "encrypt-128.png",
    "48" : "encrypt-128.png",
    "128" : "encrypt-128.png"
  },
  "browser_action": {
    "default_icon": "encrypt-128.png",
    "default_popup": "settings.html"
  },
  "content_scripts": [
    {
      "matches": ["http://vk.com/*", "https://vk.com/*"],
      "css": ["blur.css"],
      "js": ["cipher.js"]
    }
  ],
  "commands": {
    "encrypt": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Command+E"
      },
    "description": "Encrypt message"
    }
  },
  "manifest_version": 2
}
